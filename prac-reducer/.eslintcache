 useAsync(axiosFunc) {\r\n  const [state, dispatch] = useReducer(reducer, {\r\n    loading: false,\r\n    success: false,\r\n    error: false,\r\n  });\r\n\r\n  // useEffect는 비동기가 안되기 때문에 비동기 요청을 처리할 수 있는 함수를 따로 만듦 --> fetchData()\r\n  const fetchData = async () => {\r\n    // 비동기 할때는 async, await\r\n    try {\r\n      dispatch({ type: \"LOADING\" });\r\n      const data = await axiosFunc();\r\n      dispatch({ type: \"SUCCESS\", data });\r\n    } catch (e) {\r\n      dispatch({ type: \"ERROR\", error: e });\r\n    }\r\n  };\r\n  useEffect(() => {\r\n    // 컴포넌트가 마운트 되면 fetchData 실행\r\n    fetchData();\r\n  }, []);\r\n\r\n  return [state, fetchData];\r\n}\r\n","C:\\Users\\USER\\Documents\\study\\react\\new-start\\prac-reducer\\src\\components\\MovieItem.js",[],"C:\\Users\\USER\\Documents\\study\\react\\new-start\\prac-reducer\\src\\components\\Header.js",[],"C:\\Users\\USER\\Documents\\study\\react\\new-start\\prac-reducer\\src\\components\\Banner.js",[],["86","87"],"C:\\Users\\USER\\Documents\\study\\react\\new-start\\prac-reducer\\src\\components\\Home.js",[],"C:\\Users\\USER\\Documents\\study\\react\\new-start\\prac-reducer\\src\\components\\Nav.js",[],["88","89"],"C:\\Users\\USER\\Documents\\study\\react\\new-start\\prac-reducer\\src\\components\\About.js",[],["90","91"],"C:\\Users\\USER\\Documents\\study\\react\\new-start\\prac-reducer\\src\\components\\Detail.js",[],{"ruleId":"92","severity":1,"message":"93","line":8,"column":8,"nodeType":"94","messageId":"95","endLine":8,"endColumn":15},{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","replacedBy":"99"},{"ruleId":"96","replacedBy":"100"},{"ruleId":"98","replacedBy":"101"},{"ruleId":"96","replacedBy":"102"},{"ruleId":"98","replacedBy":"103"},{"ruleId":"92","severity":1,"message":"104","line":1,"column":17,"nodeType":"94","messageId":"95","endLine":1,"endColumn":27},{"ruleId":"92","severity":1,"message":"105","line":17,"column":17,"nodeType":"94","messageId":"95","endLine":17,"endColumn":24},{"ruleId":"106","se